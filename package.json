{
  "name": "epfl-news-reader",
  "private": true,
  "version": "0.0.8",
  "type": "module",
  "homepage": "https://epfl-si.github.io/EPFL-News-Reader/",
  "scripts": {
    "dev": "export epflNewsReaderEnv=local; vite",
